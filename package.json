{
  "name": "kysely-sqlite-tools",
  "type": "module",
  "version": "0.7.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "description": "",
  "author": {
    "name": "subframe7536",
    "email": "1667077010@qq.com"
  },
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -r -F \"./packages/dialect*\" dev",
    "build": "pnpm -r -F \"./packages/dialect*\" build",
    "release": "pnpm i && bumpp package.json packages/dialect*/package.json packages/plugin*/package.json",
    "serve": "cd playground && pnpm run dev",
    "publish": "pnpm -r -F \"./packages/dialect*\" publish --no-git-checks",
    "test": "pnpm run build && vitest run --root ./test --passWithNoTests",
    "test:dev": "vitest --root ./packages/sqlite-builder",
    "test:bun": "bun test test-bun",
    "typecheck": "tsc --noEmit",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "husky",
    "commit": "czg"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@sqlite.org/sqlite-wasm": "^3.48.0-build1",
    "@subframe7536/eslint-config": "^1.1.6",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^20.17.14",
    "@types/sql.js": "^1.4.9",
    "@vlcn.io/crsqlite-wasm": "^0.16.0",
    "better-sqlite3": "^11.8.0",
    "bumpp": "^9.10.1",
    "cz-git": "^1.11.0",
    "czg": "^1.11.0",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-processor-vue-blocks": "^1.0.0",
    "husky": "^9.1.7",
    "kysely": "^0.27.5",
    "lint-staged": "^15.4.0",
    "node-sqlite3-wasm": "^0.8.31",
    "sql.js": "^1.12.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
